<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cybernoodlers.virtualart"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name=".VirtualARt"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".StartupActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SelectPictureActivity"
            android:label="@string/title_activity_select_picture" >
        </activity>
        <activity
            android:name=".SetDimensionsActivity"
            android:label="@string/title_activity_set_dimensions" >
        </activity>
        <activity
            android:name=".PictureViewerActivity"
            android:label="@string/title_activity_picture_viewer" >
        </activity>
        <activity 
            android:name="org.artoolkit.ar.base.camera.CameraPreferencesActivity">
        </activity>
    </application>

</manifest>
